{
  "name": "healthcare-app",
  "version": "1.0.0",
  "description": "Health care app API and UI",
  "scripts": {
    "dev": "nodemon npm start",
    "karma": "karma start",
    "test:server": "mocha --recursive ./test/server ./test/common",
    "lint": "eslint --ext .js --ext .jsx lib src test",
    "prestart": "npm run webpack-prod",
    "start": "node src/server.js",
    "test": "npm run lint && npm run test:server && npm run karma -- --single-run",
    "watch": "npm run karma & BABEL_ENV=hot npm start & npm run watch-lint & npm run test:server",
    "watch-lint": "nodemon --watch src --watch test --ext js,jsx --exec 'npm run lint'",
    "webpack": "webpack -p",
    "webpack-noopt": "webpack",
    "webpack-prod": "webpack -p --define process.env.NODE_ENV='\"production\"'",
    "webpack-prod-noopt": "webpack --define process.env.NODE_ENV='\"production\"'",
    "tests:e2e": "nightwatch -c ./test/e2e/nightwatch.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/department-of-veterans-affairs/hca-api.git"
  },
  "keywords": [
    "connector",
    "soap"
  ],
